<?xml version="1.0"?>
<extension key="de.systopia.templateforward" type="module">
  <file>templateforward</file>
  <name>TemplateForward</name>
  <description>Extension to enable forwarding on specific pages</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Philipp Batroff</author>
    <email>batroff@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.templateforward</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.templateforward#readme</url>
    <url desc="Support">https://github.com/systopia/de.systopia.templateforward/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.3-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Simple extension to forward under certain, configgurable conditions/page hooks. See resources/example.json for configuration examples.</comments>
  <civix>
    <namespace>CRM/Templateforward</namespace>
    <format>23.02.1</format>
  </civix>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
